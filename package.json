{
  "name": "flowstate-dashboard",
  "version": "1.0.0",
  "main": "dist/main/main.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently -c \"cyan,magenta\" \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "node compile-main.cjs && npm run copy:preload && electron .",
    "copy:preload": "node -e \"require('fs').mkdirSync('dist/main/preload', {recursive: true}); require('fs').copyFileSync('src/preload/preload.js', 'dist/main/preload/preload.js')\"",
    "build": "tsc -p tsconfig.main.json && npm run copy:preload && vite build",
    "build:app": "npm run build && electron-builder",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build:native": "cd src/main/native/notification-monitor && node-gyp rebuild",
    "rebuild:native": "electron-rebuild -f -w notification-monitor",
    "postinstall": "npm run build:native || echo 'Native build skipped (optional)'"
  },
  "keywords": [
    "workspace",
    "context",
    "productivity",
    "developer-tools"
  ],
  "author": "",
  "license": "ISC",
  "description": "Flow State Dashboard - Capture and restore your development workspace context",
  "dependencies": {
    "@sentry/electron": "^7.4.0",
    "better-sqlite3": "^12.4.1",
    "chrome-remote-interface": "^0.33.3",
    "ffi-napi": "^4.0.3",
    "posthog-js": "^1.297.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.5",
    "ref-napi": "^3.0.3",
    "sql.js": "^1.13.0"
  },
  "devDependencies": {
    "@doyensec/electronegativity": "^1.10.3",
    "@electron/rebuild": "^3.7.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/chrome-remote-interface": "^0.31.14",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/sql.js": "^1.4.9",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/ui": "^4.0.15",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "electron-vite": "^4.0.1",
    "happy-dom": "^20.0.11",
    "node-addon-api": "^7.1.0",
    "node-gyp": "^10.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.15"
  }
}
